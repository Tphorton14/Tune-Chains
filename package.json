    "axios": "^0.19.0",
    "curl": "^0.1.4",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "howler": "^2.1.2",
    "if-env": "^1.0.4",
    "lodash": "^4.17.15",
    "music-metadata": "^4.6.0",
    "mysql2": "^1.7.0",
